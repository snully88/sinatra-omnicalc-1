<h1>
  Square Results
</h1>

<%= params %>
<dl>
  <dt>
    Number
  </dt>
    <dd>
      <%= params.fetch("users_number") %>
    </dd>

  <dt>
    Square
  </dt>
    <dd>
      <%= params.fetch("users_number).to_f * params.fetch("users_number").to_s
    </dd>
  </dl>

  <a href="/square/new">
    Calculate another square
  </a>
